{
   "author": "brook hong",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/16.png",
         "48": "icons/48.png"
      },
      "default_popup": "pages/popup.html",
      "default_title": "Surfingkeys"
   },
   "commands": {
      "closeTab": {
         "description": "Close the current tab."
      },
      "nextTab": {
         "description": "Go to the next tab."
      },
      "previousTab": {
         "description": "Go to the previous tab."
      },
      "proxyThis": {
         "description": "Toggle current site in autoproxy_hosts."
      },
      "restartext": {
         "description": "Restart this extenstion."
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content_scripts/content_scripts.css" ],
      "js": [ "content_scripts/modules.min.js", "content_scripts/front.js", "pages/default.js", "content_scripts/content_scripts.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "Rich shortcuts to click links/switch tabs/scroll pages or capture full page, use your browser like vim for productivity.",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Surfingkeys",
   "options_page": "pages/options.html",
   "permissions": [ "proxy", "\u003Call_urls>", "tabs", "history", "bookmarks", "storage", "sessions", "downloads", "topSites", "clipboardRead", "clipboardWrite", "tts", "downloads.shelf" ],
   "sandbox": {
      "pages": [ "pages/sandbox.html" ]
   },
   "short_name": "Rich shortcuts in vim spirit for productivity with keyboard.",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9.74",
   "web_accessible_resources": [ "pages/default.js", "pages/emoji.tsv", "pages/l10n.json", "pages/frontend.html", "pages/pdf_viewer.html", "pages/shadow.css", "pages/default.css" ]
}
